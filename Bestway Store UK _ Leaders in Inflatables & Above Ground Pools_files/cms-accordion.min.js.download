require(['jquery','accordion'],function($){$(document).ready(function(){var width=$(window).width();if($(window).width()<768){accordionSetup();cmsAccordion();}});function accordionSetup(){var accordionElements=$('.magezon-builder .columns-3-grey-container');if(accordionElements.size()===3){accordionElements.wrapAll("<div class='accordion-content' data-role='content'></div>");var accordionContent=$('.magezon-builder .accordion-content');accordionContent.before('<div class="title" data-role="collapsible">'+'    <div data-role="trigger">'+'      Quick Links'+'    </div>'+'</div>');accordionContent=accordionContent.add(accordionContent.siblings('.title'));accordionContent.wrapAll("<div class='cms-accordion'></div>");}}
function cmsAccordion(){var currentAccordion=$(".cms-accordion");currentAccordion.accordion({active:[],animate:{duration:400},icons:{"header":"plus","activeHeader":"minus"},"collapsible":true,"multipleCollapsible":true});currentAccordion.find('span').on('click',function(event){$(this).siblings('div[data-role="trigger"]').click();event.preventDefault();event.stopPropagation();});}})